SIMULATE 1

perf FUNCTION RN1,R
0 0.8
1 0.2
queue CAPACITY 6
CAM CAPACITY 63
PACK	GENERATE 8.64,,0,10000
START	LET PRIORITY=fn$perf
	IF PRIORITY=1,pCr1
	ARRIVE crono
vuelta	GOTO PESA,0.01
MAQ	ENTER queue,Q
	ADVANCE 45*FN$XPDIS
	LEAVE queue
cro	IF PRIORITY=1,pCr2
	DEPART crono
SEPA	GOTO PTO2,0.5
	ASSEMBLE 40
	SEIZE PTO1,Q
	ADVANCE 600
	RELEASE PTO1
	GOTO CAM
PTO2	ASSEMBLE 40
	SEIZE PTO2,Q
	ADVANCE 600
	RELEASE PTO2
CAM	ENTER CAM,1,Q
	ADVANCE 21600
	LEAVE CAM,1
TERM	TERMINATE 0

TIME	GENERATE 86400
	TERMINATE 1
PESA	ENTER queue,Q
	ADVANCE 45*FN$XPDIS*1.5
	LEAVE queue
	GOTO cro
pCr1	ARRIVE pCr1
	GOTO vuelta
pCr2	DEPART pCr1
	GOTO SEPA

START 1
END
